      <ul>
        <% trn_budget_detail.errors.full_messages.each do |trn_budget_detail| %>
          <li><%= trn_budget_detail %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <%= f.label :PJID, 'PJID' %><br>
  <%= f.text_field :pj_id %><br>
  <%= f.label :subpjID, 'subpjID' %><br>
  <%= f.text_field :subpj_id %><br>
  <%= f.label :teamID, 'チームID' %><br>
  <%= f.text_field :team_id %><br>

  <%= f.label :PJname, 'PJname' %><br>
