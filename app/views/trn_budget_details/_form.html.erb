  <%= f.text_field :subpj_name %><br>
  <%= f.label :teamname, 'チームname' %><br>
  <%= f.text_field :team_name %><br>


  <%= f.label :bgtitle, '予算タイトル' %><br>
  <%= f.text_field :bg_title %><br>

  <%= f.label :bgkind, '予算種別' %><br>
  <%= f.text_field :bg_kind %><br>

